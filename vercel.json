{
    "installCommand": "pip3.12 install -r requirements.txt",
    "devCommand": "uvicorn api.app:app --host 0.0.0.0 --port 3000",
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ]
  }
  